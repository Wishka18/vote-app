<form method="post" action="{{ path('app_member_delete', {'id': member.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ member.id) }}">
    <button class="crud-prefix-btn crud-prefix-btn-delete" title="Supprimer">
        <i class="fas fa-trash-alt"></i>
    </button>
</form>
